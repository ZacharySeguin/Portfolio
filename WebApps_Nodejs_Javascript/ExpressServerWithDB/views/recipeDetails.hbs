<!DOCTYPE html>
<html>
<head>
  <title> {{title}} </title>
</head>
<body>
   <h1>{{title}}</h1>
<div>
    {{#each recipeEntries}}
      <h1>{{this.recipe_name}}</h1>
      <p>Contributor: {{this.contributor}}</p>
      <p>Category: {{this.category}}</p>
      <p>Description: {{this.description}}</p>
      <p>Spices: {{this.spices}}</p>
      <p>Source: {{this.source}}</p>
      <p>Rating: {{this.rating}}</p>
      <p>Ingredients: {{this.ingredients}}</p>
      <p>Directions: {{this.directions}}</p>
	{{/each}}
</div>
</body>
</html>
